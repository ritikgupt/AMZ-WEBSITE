 <script>

  $(function() {
    // Initialize form validation on the registration form.
    // It has the name attribute "registration"


    $("form[name='getInTouchForm']").validate({
      // Specify validation rules
      rules: {
        // The key name on the left side is the name attribute
        // of an input field. Validation rules are defined
        // on the right side
        name: {
          required: true,
          // Specify that Name should be validated
          // by the built-in "Name" rule
          minlength: 5
        },
        email: {
          required: true,
          // Specify that email should be validated
          // by the built-in "email" rule
          email: true
        },

              phone: {
          required: true,
          // Specify that email should be validated
          // by the built-in "email" rule
          minlength: 10,
          maxlength: 10
        },

        message: {
          required: true,
          // Specify that Name should be validated
          // by the built-in "Name" rule
          minlength: 10
        }	

      },
      // Specify validation error messages
      messages: {
        name: "Please enter your Name",
        email: "Please enter a valid Email address",
        phone: "Phone number required 10 digit",
        message:"Minimum 10 characters required"
      },
      // Make sure the form is submitted to the destination defined
      // in the "action" attribute of the form when valid
      submitHandler: function(form) {
        form.submit();
      }
    });

    $("form[name='certificateValidate-Form']").validate({
      rules: {

        certificateId: {
          required: true,
          minlength: 5
        }

      },
      messages: {
        certificateId:"Invalid Serial Number"
      },
      submitHandler: function(form) {
        form.submit();
      }
    });

    $("form[name='newsLetter-Form']").validate({
      rules: {

        email: {
          required: true,
          email: true
        },

      },
      messages: {
        email: "Not a valid Email"
      },
      submitHandler: function(form) {
        form.submit();
      }
    });

  });

</script>

